{
  "manifest_version": 2,

  "name": "Web Bhumi Plugin",
  "description": "This extension will open Web Bhumi in plugin",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "demo.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
       {
         "matches": ["http://*/*"],
         "js": ["popup.js"]
       }
     ]
}
